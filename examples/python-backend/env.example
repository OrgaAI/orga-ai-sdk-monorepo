# OrgaAI Configuration
# Copy this file to .env and fill in your actual values

# Your OrgaAI API key (get this from the OrgaAI dashboard)
REALTIME_USER_TOKEN=your_orga_api_key_here

# Your developer email address
REALTIME_USER_EMAIL=developer@example.com

# Optional: Enable debug logging
DEBUG=false

# Optional: Custom API base URL (defaults to https://api.orga-ai.com)
# ORGA_BASE_URL=https://api.orga-ai.com
