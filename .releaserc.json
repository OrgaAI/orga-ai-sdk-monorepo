{
    "branches": [
      { "name": "main", "channel": "latest" },
      { "name": "dev", "channel": "beta", "prerelease": "beta" },
      { "name": "ci-cd/test", "channel": "test", "prerelease": "test"},
      {"name": "sdk-test/*", "channel": "experimental", "prerelease": "experimental"}
    ],
    "plugins": [
      ["@semantic-release/commit-analyzer", { "preset": "conventionalcommits" }],
      ["@semantic-release/release-notes-generator", { "preset": "conventionalcommits" }],
      ["@semantic-release/npm", { "npmPublish": true, "pkgRoot": "packages/sdk-web", "publishConfig": { "access": "restricted" } }],
      ["@semantic-release/npm", { "npmPublish": true, "pkgRoot": "packages/sdk-react-native", "publishConfig": { "access": "restricted" } }],
      ["@semantic-release/git", {
        "assets": [
          "packages/sdk-web/package.json",
          "packages/sdk-web/CHANGELOG.md",
          "packages/sdk-react-native/package.json",
          "packages/sdk-react-native/CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }]
    ],
    "extends": "semantic-release-monorepo"
  }